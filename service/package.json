{
  "name": "service",
  "version": "0.2.6",
  "main": "build/index.js",
  "private": true,
  "license": "MIT",
  "scripts": {
    "bundle:api": "swagger-cli bundle -t yaml -o build/api/api-spec.yaml src/api/api-spec.yaml",
    "build": "rimraf ./build && tsc && yarn run bundle:api",
    "pretest": "yarn run bundle:api",
    "start": "node -r dotenv/config build/index.js",
    "dev": "nodemon",
    "test": "jest --runInBand",
    "test:ci": "jest --ci --coverage --coverageReporters=text --coverageReporters=text-summary",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@dhruv-m-patel/express-app": "workspace:*"
  },
  "devDependencies": {
    "@apidevtools/swagger-cli": "^4.0.4",
    "@types/jest": "^29.5.14",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "nodemon": "^2.0.6",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.3",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.0.0",
    "typescript": "^5.7.3"
  }
}
